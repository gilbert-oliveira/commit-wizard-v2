# Configuração do Commit Wizard
# Copie este arquivo para .env e configure suas chaves

# OpenAI API Key (obrigatório)
OPENAI_API_KEY=sua_chave_openai_aqui

# Codecov Token (para Coverage Analytics)
CODECOV_TOKEN=seu_token_codecov_aqui

# Configurações opcionais
COMMIT_WIZARD_DEBUG=false
COMMIT_WIZARD_DRY_RUN=false

# Configurações do Codecov (opcional)
CODECOV_SLACK_WEBHOOK=https://hooks.slack.com/services/...
CODECOV_WEBHOOK_URL=https://seu-webhook.com/coverage 